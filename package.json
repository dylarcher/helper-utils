{
	"author": "Dylan Archer <dylarcher@gmail.com> (@dylarcher)",
	"bugs": {
		"url": "https://github.com/dylarcher/js.helper-utils/issues"
	},
	"description": "Reusable javascript helper utility methods.",
	"optionalDependencies": {
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-prettier": "5.4.1",
		"eslint-plugin-yaml": "1.0.3",
		"@stedi/prettier-plugin-jsonata": "2.1.3",
		"prettier-plugin-glsl": "0.2.1",
		"prettier-plugin-properties": "0.3.0"
	},
	"engines": {
		"node": ">=18.20.8",
		"npm": ">=10.9.2"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/types.d.ts"
		},
		"./*": {
			"import": "./dist/*.js",
			"require": "./dist/*.js",
			"types": "./dist/types.d.ts"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"homepage": "https://dylarcher.github.io/js.helper-utils/",
	"keywords": [
		"helper",
		"utils",
		"utilities",
		"javascript",
		"library",
		"methods",
		"functions"
	],
	"license": "MIT",
	"main": "./dist/index.js",
	"name": "js-helpers",
	"packageManager": "npm@10.9.2",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dylarcher/js.helper-utils.git"
	},
	"type": "module",
	"scripts": {
		"build": "npx tsc --minify",
		"bundle": "npx dts-bundle-generator --config=./.github/linters/dts.config.js",
		"bundle:watch": "npm run bundle --watch",
		"prettier": "npx prettier --config=./.github/linters/.prettierrc.yml . --ignore-unknown",
		"format:check": "npm run prettier --check",
		"format": "npm run prettier --write",
		"eslint": "npx eslint . --config=./.github/linters/eslint.config.js --ext .js --report-unused-disable-directives",
		"lint:check": "npm run eslint",
		"lint": "npm run eslint -- --fix",
		"prepare": "npm dedupe && npm prune",
		"prebuild": "npm run lint:check && npm run format:check",
		"prepublishOnly": "npm run build && npm run dts",
		"pretest": "npm run build && npm run dts",
		"test": "node --no-warnings --experimental-specifier-resolution=node --inspect-brk test --reporter=spec ./**/*.test.js",
		"c8": "npx c8 --config=./.github/linters/.c8rc.json",
		"test:coverage": "npm run c8 -- npm test",
		"test:watch": "npm test --watch"
	},
	"types": "./dist/types.d.ts",
	"version": "0.0.1"
}
