{
    "all": false,
    "branches": 100,
    "check-coverage": true,
    "extensions": [
        ".js"
    ],
    "src": true,
    "exclude": [
        "node_modules/**/*",
        "coverage/**/*"
        "utils/**/*",
        "dist/**/*",
        "src/**/*.spec.js",
        "src/**/*.test.js",
    ],
    "clean": true,
    "merge-async": true,
    "experimental-monocart": true,
    "omit-relative": false,
    "functions": 100,
    "include": [
        "src/**/*.js"
    ],
    "lines": 100,
    "report-dir": "coverage",
    "reporter": [
        "json-summary", 
        "text-summary", 
        "html", 
        "lcov", 
        "json", 
        "text", 
        "clover",
        "cobertura",
        "teamcity", 
        "terraform"
    ],
    "statements": 100
}