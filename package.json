{
    "author": "Dylan Archer <dylarcher@gmail.com> (@dylarcher)",
    "bugs": {
        "url": "https://github.com/dylarcher/js.helper-utils/issues"
    },
    "description": "Reusable javascript helper utility methods.",
    "devDependencies": {
        "@stedi/prettier-plugin-jsonata": "2.1.3",
        "@types/node": "24.0.1",
        "c8": "10.1.3",
        "eslint": "9.28.0",
        "eslint-config-prettier": "10.1.5",
        "eslint-plugin-markdown": "5.1.0",
        "eslint-plugin-prettier": "5.4.1",
        "eslint-plugin-yaml": "1.0.3",
        "prettier": "3.5.3",
        "prettier-plugin-glsl": "0.2.1",
        "prettier-plugin-properties": "0.3.0",
        "rimraf": "6.0.1",
        "typescript": "5.8.3"
    },
    "overrides": {
        "glob": "^10.4.5",
        "inflight": "npm:lru-cache@^10.2.0"
    },
    "engines": {
        "node": ">=22",
        "npm": ">=11"
    },
    "packageManager": "npm@11.4.1",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/types.d.ts"
        },
        "*": {
            "import": "./dist/*.js",
            "require": "./dist/*.js",
            "types": "./dist/types.d.ts"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "homepage": "https://dylarcher.github.io/js.helper-utils/",
    "keywords": [
        "helper",
        "utils",
        "utilities",
        "javascript",
        "library",
        "methods",
        "functions"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "name": "js-helpers",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dylarcher/js.helper-utils.git"
    },
    "scripts": {
        "build": "rimraf dist && tsc",
        "c8": "c8 --config .github/presets/.c8rc.json",
        "eslint": "eslint --config .github/presets/eslint.config.js --ext .js --report-unused-disable-directives",
        "format": "npm run prettier --write",
        "format:check": "npm run prettier --check",
        "lint": "npm run eslint -- --fix",
        "lint:check": "npm run eslint",
        "prebuild": "npm run lint:check && npm run format:check",
        "prepare": "npm dedupe && npm prune",
        "prepublishOnly": "npm run build",
        "pretest": "npm run build",
        "prettier": "prettier . --config .github/presets/.prettierrc.yml --ignore-unknown",
        "test": "node --no-warnings --test",
        "test:coverage": "npm run c8 -- npm test",
        "test:watch": "npm test --watch"
    },
    "type": "module",
    "types": "dist/types.d.ts",
    "version": "0.0.1"
}
