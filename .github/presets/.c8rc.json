{
    "all": true,
    "branches": 100,
    "check-coverage": false,
    "exclude": [
        "node_modules/**/*.{js,ts}"
        "utils/**/*.js",
        "src/**/*.spec.js",
        "src/**/*.test.js"
    ],
    "functions": 100,
    "include": [
        "src/**/*.js"
    ],
    "lines": 100,
    "report-dir": "dist/.coverage",
    "reporter": [
        "text",
        "html",
        "lcov"
    ],
    "statements": 100
}